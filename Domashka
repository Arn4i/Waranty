public class Vehicle {
    public Vehicle(){
        model = "Tesla";
        maxspeed = 320;
        isDieselFuel = false;
        fuel = 0;
    }
    String model;
    int maxspeed;
    boolean isDieselFuel;
    int fuel;


    public String getModel(){
        return model;
    }
    void drive() {
        fuel -=1;
        System.out.println("Заводит....");
    }
    void stop(){
        fuel = fuel;
        System.out.println("*Остановился*");
    }
    void refuel(){
        fuel = 100;
        System.out.println("*Заряжается*");
    }


    public static void main(String[] args){
        Vehicle vehicle = new Vehicle();

    }
}
